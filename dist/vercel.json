{
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/privacypolicy",
      "destination": "/privacypolicy.html"
    },
    {
      "source": "/privacy",
      "destination": "/privacypolicy.html"
    },
    {
      "source": "/termscondition",
      "destination": "/terms.html"
    },
    {
      "source": "/terms",
      "destination": "/terms.html"
    },
    {
      "source": "/cookiespolicy",
      "destination": "/cookies.html"
    },
    {
      "source": "/cookies",
      "destination": "/cookies.html"
    },
    {
      "source": "/deletedata",
      "destination": "/deletedata.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}