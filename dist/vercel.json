{
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/privacy-policy",
      "destination": "/privacypolicy.html"
    },
    {
      "source": "/privacy",
      "destination": "/privacypolicy.html"
    },
    {
      "source": "/terms-condition",
      "destination": "/terms.html"
    },
    {
      "source": "/terms",
      "destination": "/terms.html"
    },
    {
      "source": "/cookies-policy",
      "destination": "/cookies.html"
    },
    {
      "source": "/cookies",
      "destination": "/cookies.html"
    },
    {
      "source": "/deletedata",
      "destination": "/deletedata.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}